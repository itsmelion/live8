{
  "name": "alive8",
  "version": "2.0.0",
  "description": "theme for live8",
  "scripts": {
    "up": "npm upgrade && npm prune && npm dedupe",
    "precommit": "lint-staged",
    "push": "rsync -avz ./build/ lion@alia.ml:/home/lion/sites/live8/ --progress --delete",
    "deploy": "npm run build && npm run push",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "flamework": "^3.0.10",
    "pace-js": "^1.0.2",
    "popper.js": "^1.14.4",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-swipeable": "^4.3.0"
  },
  "devDependencies": {
    "node-sass": "^4.10.0"
  },
  "optionalDependencies": {
    "eslint": "^5",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "stylelint": "^9.7.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.4.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.15.1"
  },
  "main": "src/index.js",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:itsmelion/live8.git"
  },
  "author": "Christhopher Lion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itsmelion/live8/issues"
  },
  "homepage": "https://live8.alia.ml"
}
